#!/bin/bash

# List IAM Users
#


# $ iam-users
iam-users() {

#	if [[ "$@" -lt 2 ]]; then
#		show_usage
		exit 1
#	fi

#	if [[ $1 == "-c" ]]; then
		validate_file "$2"
#	else 
#		show_usage
#		exit 1
#	fi

	aws iam list-users    \
    	--output text       \
    	--query '
      	Users[].[
        	UserName,
        	UserId,
        	CreateDate
    	]'
 }

